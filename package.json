{
  "name": "@tinacms/starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "TINA_PUBLIC_IS_LOCAL=true tinacms dev -c \"next dev\"",
    "dev:prod": "TINA_PUBLIC_IS_LOCAL=false tinacms dev -c \"next dev\"",
    "build": "TINA_PUBLIC_IS_LOCAL=false tinacms build && next build",
    "build:dev": "TINA_PUBLIC_IS_LOCAL=true tinacms build && next build",
    "start": "tinacms build && next start",
    "export": "npm run build && next export",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "next lint --fix && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.3.1",
    "@tinacms/cli": "^1.5.42",
    "@types/aos": "^3.0.7",
    "@types/js-cookie": "^3.0.0",
    "@types/node": "^20.10.6",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^18.2.18",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.13.0",
    "postcss": "^8.3.11",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^10.1.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.8",
    "@tinacms/auth": "^1.0.4",
    "@tinacms/datalayer": "^1.2.32",
    "aos": "^2.3.4",
    "date-fns": "^2.23.0",
    "next": "^13.3.1",
    "next-auth": "^4.24.5",
    "next-tinacms-s3": "^1.3.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.2.0",
    "styled-jsx": "^3.2.5",
    "swiper": "8.4.7",
    "tailwindcss": "^3.2.4",
    "tinacms": "^1.5.28",
    "tinacms-authjs": "^1.0.1",
    "tinacms-gitprovider-github": "^1.0.3",
    "typescript": "^5.3.3",
    "upstash-redis-level": "^1.1.1"
  }
}
